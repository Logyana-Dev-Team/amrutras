                align-items-center
                text-center text-sm-left
              "
            >
              <a
                class="d-inline-block mx-auto mr-sm-4"
                href="/product-single/<%= cart.productId %>"
                style="width: 10rem"
                ><img src="<%=cart.img[0].filename%>" alt="Product"
              /></a>
              <div class="media-body pt-2">
                <h3 class="product-title font-size-base mb-2">
                  <a href="/product-single/<%= cart.productId %>"
                    ><%= cart.name %></a
                  >
                </h3>
                <div class="font-size-lg text-accent pt-2">
                  ₹<%= cart.price %>.<small>00</small>
                </div>
              </div>
            </div>
            <div
              class="
                pt-2 pt-sm-0
                pl-sm-3
                mx-auto mx-sm-0
                text-center text-sm-left
