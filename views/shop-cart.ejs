    <!-- Page Content-->
    <div class="container pb-5 mb-2 mb-md-4">
      <div class="row" id="cartSection">
        <% if(login){ %>
        <!-- List of items-->
        <section class="col-lg-8">
          <div
            class="
              d-flex
              justify-content-between
              align-items-center
              pt-3
              pb-2 pb-sm-5
              mt-1
            "
          >
