              <div class="col-sm-6">
                <div class="form-group">
                  <label for="reg-password">Password</label>
                  <input
                    class="form-control"
                    type="password"
                    required
                    id="reg-password"
                  />
                  <div class="invalid-feedback">Please enter password!</div>
                </div>
